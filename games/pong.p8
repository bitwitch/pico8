pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
 p={}
 p.x=64
 p.y=115
 p.w=25
 p.h=2
 p.s=2
 
 b={}
 b.x=10
 b.y=0
 b.z=2
 b.vx=1
 b.vy=2
end

function upd8_paddle()
	if(btn(0)) p.x-=p.s
	if(btn(1)) p.x+=p.s
	if(p.x+p.w > 127) p.x=127-p.w
	if(p.x < 0) p.x=0
end

function upd8_ball()
	if(b.x+b.z > 127) then 
		b.x=127-b.z
		b.vx=-1
		sfx(0)
	elseif(b.x<0) then 
		b.x=0
		b.vx=1
		sfx(0)
	end
	
	if(b.y+b.z > 127) then 
		b.y=127-b.z
		b.vy=-2
		sfx(0)
	elseif(b.y<0) then 
		b.y=0
		b.vy=2
		sfx(0)
	end
	
	if(above()) then 
		if (b.y+b.z+b.vy >= p.y) then
			b.vy=-2
			sfx(0)
		end
	end
	
	b.x+=b.vx
	b.y+=b.vy
end

function above()
	return b.x+b.z>p.x and b.x<p.x+p.w and b.y+b.z<p.y
end

function _update() 
	upd8_paddle()
	upd8_ball()
end

function _draw()
	cls()	
	rectfill(p.x,p.y,p.x+p.w,p.y+p.h,7)
 rectfill(b.x,b.y,b.x+2,b.y+2,8)
end
