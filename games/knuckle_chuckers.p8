pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
	p1={
		frame=1,
		spr=1,
		x=32, y=64,
		hitbox={x=4,y=5,w=6,h=19},
		update=function(this)
			this.frame+=1
			--animation
			if this.frame>=15 then
				this.spr=this.spr==1 and 3 or 1
				this.frame=1
			end
		end,
		draw=function(this)
			spr(this.spr,this.x,this.y,2,3)
		end
	}
	
	p2={
		frame=1,
		spr=7,
		x=32, y=64,
		hitbox={x=4,y=5,w=6,h=19},
		update=function(this)
			this.frame+=1
			--animation
			if this.frame>27 then
				this.spr=134
				this.frame=1
			elseif this.frame>21 then
				this.spr=136
			elseif this.frame>14 then
			 this.spr=140
			elseif this.frame>7 then
				this.spr=136
			end
		end,
		draw=function(this)
			spr(this.spr,this.x,this.y,2,3)
		end
	}
	
	--globals
	tmr=0
	started=false
	current_level=1
end

function _update()
	p1.update(p1)
	p2.update(p2)
	tmr=(tmr+1)%30
end

function _draw()
	cls(1)
	if not started then
		start_screen()
	else 
		load_level(current_level)
		p1.draw(p1)
	end
end

function start_screen()
	--floor
	--rectfill(0,87,127,127,8)
	--title
	spr(64,64-8*4,17,8,2) 
	--dudes
	spr(p1.spr,32-8,64,2,3)
	pal(1,0)
	spr(p2.spr,96-8,64,2,3,true)
	pal()
	--text
	if tmr>=14 then
		print("press z-x",64-4*5,112,6)
	end
	if btn(4) or btn(5) then
		started=true
	end
end

function load_level(num)
	if num==1 then
		
	elseif num==2 then
	elseif num==3 then
	end
end
