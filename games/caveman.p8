--entities

-- creates a "class" object
-- with support for basic
-- inheritance/initialization
function kind(kob)
 kob=kob or {}
 setmetatable(kob,{__index=kob.extends})

 kob.new=function(self,ob)
  ob=set(ob,{kind=kob})
  setmetatable(ob,{__index=kob})
  if (kob.create) ob:create()
  return ob
 end

 return kob
end

function update_with_table(e,fn)
	for prop,list in pairs(entities_with) do
		if e[prop] then
			fn(list,e)
		end
	end
end

-- entity type
entity=kind()
eid=1
function entity:create()
	if not self.name then
		self.name=eid..""
		eid+=1
	end
	entities[self.name]=self
	update_with_table(self,add)
end


--collisions
function test_collisions(ent,prop,cb)
	add(cqueue,{e=ent,p=prop,cb=cb})
end

function do_collisions()
 for c in all(cqueue) do
  local e=c.e
  for o in all(entities_with[c.p]) do
   if o~=e then
    if overlap(e,o) then
    	extcmd("pause")
    end
   end
  end
 end
 cqueue={}
end


function init_world()
	for x=0,36 do
		for y=0,17 do
			s=mget(x,y)
			--collision flag
			if fget(s,0) then
				local e=entity:new({
					x=x+8,
					y=y+16,
					w=16,
					h=16,
					collide=true,
				})
			end
		end
	end
end

