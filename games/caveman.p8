pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
--caveman
--by bitwitch
man={}
fl_h=24
grav=.45
particles={}
cam={}
cam.x=0
cam.y=0
map_objs={}
debug=true

--don't repeat button presses
poke(0x5f5c,255)

function _init()
	init_man()
end

function _update()
 update_particles()
 update_man()
 update_cam()
end

function _draw()
 cls()
 draw_particles()
 --draw_floor()
 draw_map()
 draw_fire()
 draw_man()
end

function update_cam()
	cam.x=lerp(cam.x,man.x-64,.1)		
	if cam.x<0 then cam.x=0 end
	camera(cam.x,cam.y)
end

function draw_map()
	map(0,0,0,0,64,16)
end

function draw_floor()
 rectfill(0,128-fl_h,128,128,4)
end

function draw_fire()
	local x=35
	spr(47,x,128-fl_h-8)
	make_flames(x+4,128-fl_h,5)
end

function lerp(a,b,amt)
	return (1-amt)*a+b*amt
end

function collide(a,b)
    --top left
	if a.x-.5*a.w >= b.x-.5*b.w and
	   a.x-.5*a.w <= b.x+.5*b.w and
	   a.y-a.h    >= b.y-b.h    and
	   a.y-a.h    <= b.y        and
	   --top right
	   a.x+.5*a.w >= b.x-.5*b.w and
	   a.x+.5*a.w <= b.x+.5*b.w and
	   a.y-a.h    >= b.y-b.h    and
	   a.y-a.h    <= b.y        and
	   --bottom left
	   a.x-.5*a.w >= b.x-.5*b.w and
	   a.x-.5*a.w <= b.x+.5*b.w and
	   a.y        >= b.y-b.h    and
	   a.y        <= b.y        and
	   --bottom right
	   a.x+.5*a.w >= b.x-.5*b.w and
	   a.x+.5*a.w <= b.x+.5*b.w and
	   a.y        >= b.y-b.h    and
	   a.y        <= b.y        
	then
		return true
	end
	return false
end


