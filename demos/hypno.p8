pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
function _init() 
	mx=92
	mn=1
	c=1
	tmr=0
	st=0
end

function _update()
	mn+=1
	mx+=1
	c+=1
	
	if (tmr==30*3) then
	 st=1 
	 tmr+=1
	end
	
	if (tmr<30*3) tmr+=1
	
	if (mx > 32000) then
	 mn=1
	 mx=92
	end
	
	if(c>15) c=1
end

function _draw() 
	cls()
	if(st==0) then
		idle()
	elseif(st==1) then
		plug_in()
	end
end

function idle() 
	sspr(8,0,16,32,32,0,64,128)
end

function plug_in()
 for i=mn,mx do
		circfill(64,64,mx-i,i)
	end
	
	pal(7,0)
	sspr(24,0,16,32,32,0,64,128)
	pal()
end


