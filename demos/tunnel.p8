pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
w=128
h=128
cx=0.5*w
cy=0.5*h

x={}
y={}
z={}

pl={1,12,1,
				2,1,2,
				1,14,1,
				2,1,2}

n=270 -- num circles

function _init()
	music(0)
end

function _draw()
	cls()
	local t=time()
	local phase,xi,yi,r,col
	local depth_factor,time_factor
	for zi=n,0,-1 do
		phase=0.0015*zi
  xi=.5*w
			  *cos(0.06*t+phase)
			  *cos(0.12*t+phase)
			  +.5*w
  yi=.5*h
			  *sin(0.05*t+phase)
			  *sin(0.1*t+phase)
			  +.5*h
  r=0.5*(n-zi)+10
  
		depth_factor=sqrt(4*(n-zi))
  time_factor=-t*14
  col=flr((depth_factor+time_factor+.5)/2)%#pl+1
  
  circ(xi,yi,r,pl[col])
	end
end

