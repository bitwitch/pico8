pico-8 cartridge // http://www.pico-8.com
version 19
__lua__
-- times tables
-- bitwitch

pi=3.14159256
r=62
w=128
h=128
points={}

table = 1
lerpamt = 0

function _init()
	generatepoints(100)
end

function generatepoints(n)
	local i=0
	for angle=0,2*pi,2*pi/n do
	 local x=round(r*cos(angle/(2*pi)))
	 local y=round(r*sin(angle/(2*pi)))
	 points[i]={x=.5*w+x,y=.5*h+y}
	 i+=1
	end
end

function round(n)
	return flr(n+.5)
end

function lerp(a,b,amt)
	return a*(1-amt)+b*amt
end

function drawlines(table,lerpamt)
	local prod,prodnext,angle,anglenext,angledraw
	for i=0,#points do 
  prod= (i*table) % #points
  prodnext = (i*(table+1)) % #points
  angle = 2*pi/#points*prod
  anglenext = 2*pi/#points*prodnext

  if angle<anglenext then
  	anglenext=anglenext-2*pi;
  end

  angledraw=lerp(angle,anglenext,lerpamt);

  destx=r*cos(angledraw/(2*pi))+0.5*w;
  desty=r*sin(angledraw/(2*pi))+0.5*h;

  line(points[i].x,points[i].y,destx,desty,12)
	end
end

function _update()
	lerpamt+=0.008; 
 if lerpamt>=1 then
 	table=(table+1) % 300
 	lerpamt=0
	end
end

function _draw()
	cls()
	drawlines(table,lerpamt)
	circ(0.5*w,0.5*h,r,12)
	circ(0.5*w,0.5*h,r+1,12)
end
